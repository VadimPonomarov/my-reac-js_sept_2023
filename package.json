{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@apollo/client": "^3.8.9",
    "@csstools/normalize.css": "^12.0.0",
    "@emotion/react": "^11.11.3",
    "@emotion/styled": "^11.11.0",
    "@fontsource/roboto": "^5.0.8",
    "@hookform/resolvers": "^3.3.4",
    "@mui/icons-material": "^5.15.2",
    "@mui/material": "^5.15.2",
    "@reduxjs/toolkit": "^2.0.1",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.5.2",
    "@types/node": "^16.18.68",
    "@types/react": "^18.2.45",
    "@types/react-dom": "^18.2.18",
    "@types/react-redux": "^7.1.33",
    "axios": "^1.6.7",
    "framer-motion": "^10.18.0",
    "graphql": "^16.8.1",
    "moment": "^2.30.1",
    "process": "^0.11.10",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.49.3",
    "react-query": "^3.39.3",
    "react-redux": "^9.0.4",
    "react-router-dom": "^6.21.2",
    "react-scripts": "5.0.1",
    "redux-persist": "^6.0.0",
    "sass": "^1.69.5",
    "typescript": "^4.9.5",
    "usehooks-ts": "^2.9.5",
    "web-vitals": "^2.1.4",
    "yup": "^1.3.3"
  },
  "devDependencies": {
    "@babel/plugin-transform-private-property-in-object": "^7.23.4",
    "@graphql-codegen/cli": "^5.0.0",
    "@graphql-codegen/client-preset": "^4.1.0",
    "@parcel/watcher": "^2.3.0",
    "@reduxjs/rtk-codemods": "^0.1.0",
    "@types/css-modules": "^1.0.5",
    "@types/graphql": "^14.5.0",
    "@types/react-router-dom": "^5.3.3",
    "@types/uuid": "^9.0.7",
    "generate-react-cli": "^8.4.0",
    "postcss-normalize": "^10.0.1",
    "@types/moment": "^2.13.0",
    "ts-import": "^0.0.34-4",
    "ts-node": "^10.9.2"
  },
  "scripts": {
    "start": "npm run fix && react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "fix": "eslint src/ --fix",
    "component": "generate-react component",
    "layout": "generate-react component --type=layout",
    "page": "generate-react component --type=page",
    "slice": "generate-react component --type=slice",
    "form": "generate-react component --type=form",
    "formSubcomponent": "generate-react component --type=formSubcomponent",
    "redux:utils:createSlice": "npx rtk-codemods createSliceBuilder ./src/store",
    "redux:utils:createSliceReducer": "npx rtk-codemods createSliceReducerBuilder ./src/store/ --filename.ts",
    "gql:cg": "graphql-codegen",
    "gql:cgw": "graphql-codegen --watch"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "rules": {
      "no-console": "off",
      "no-unused-vars": "off",
      "@typescript-eslint/no-unused-vars": [
        "off"
      ],
      "import/order": [
        "error",
        {
          "groups": [
            "builtin",
            "external",
            "internal"
          ],
          "pathGroups": [
            {
              "pattern": "react",
              "group": "external",
              "position": "before"
            }
          ],
          "pathGroupsExcludedImportTypes": [
            "react"
          ],
          "newlines-between": "always",
          "alphabetize": {
            "order": "asc",
            "caseInsensitive": true
          }
        }
      ]
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
